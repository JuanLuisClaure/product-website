{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["eslint:recommended","airbnb-base"],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "rules": {

        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console": [
            "error"
        ]
    }
}
